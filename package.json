{
	"name": "booking-api",
	"version": "1.0.50",
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"dev-ldap": "export NODE_TLS_REJECT_UNAUTHORIZED='0' && bun run dev",
		"build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/index.ts ",
		"lint": "biome check . && biome format .",
		"format": "biome check --write . && biome format . --write"
	},
	"dependencies": {
		"@elysiajs/bearer": "^1.1.2",
		"@elysiajs/cors": "^1.1.1",
		"@elysiajs/cron": "^1.1.1",
		"@elysiajs/eden": "^1.1.3",
		"@elysiajs/jwt": "^1.2.0",
		"@elysiajs/static": "^1.2.0",
		"@elysiajs/swagger": "^1.1.6",
		"@fontsource/kanit": "^5.2.6",
		"@google-cloud/translate": "^9.2.0",
		"@prisma/client": "^6.8.2",
		"@types/nodemailer": "^6.4.17",
		"aws-sdk": "^2.1692.0",
		"axios": "^1.9.0",
		"bcryptjs": "^3.0.2",
		"dotenv": "^16.5.0",
		"elysia": "^1.1.25",
		"expo-server-sdk": "^3.12.0",
		"jsonwebtoken": "^9.0.2",
		"ldap-authentication": "^3.3.4",
		"ldapjs": "^3.0.7",
		"mock-aws-s3": "^4.0.2",
		"moment": "^2.30.1",
		"mysql2": "^3.11.5",
		"nock": "^14.0.5",
		"nodemailer": "^7.0.4",
		"prisma": "^6.8.2",
		"wordpress-hash-node": "^1.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bcrypt": "^5.0.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/bun": "^1.1.14",
		"@types/jsonwebtoken": "^9.0.8",
		"@types/ldapjs": "^3.0.6",
		"@types/mysql": "^2.15.26",
		"@types/node-cron": "^3.0.11",
		"@types/wordpress-hash-node": "^1.0.2"
	},
	"module": "src/index.js"
